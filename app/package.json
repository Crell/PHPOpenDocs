{
  "name": "app",
  "version": "1.0.0",
  "description": "An example of all the Imagick functions. Or at least most of them. The site is hosted at http://phpimagick.com/",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "./test"
  },
  "scripts": {
    "test": "echo \\\"Error: no ../test specified\\\" && exit 1",
    "start": "webpack-dev-server --open",
    "build:dev": "webpack --mode development",
    "build:dev:watch": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "build:watch": "webpack --watch --mode production",
    "stats": "webpack --profile",


    "sass:build": "node-sass --source-map true scss/ -o public/css",
    "sass:build:watch": "node-sass --source-map true --watch scss/ -o public/css",
    "build-task-wat:autoprefixer": "postcss dist/css/*.css --use autoprefixer -d public/css",
    "sass-wat:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'scss/**/*.scss' -c 'npm run sass:build'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlwaysSeptemeber/Voting/issues"
  },
  "homepage": "https://github.com/AlwaysSeptemeber/Voting",
  "dependencies": {
    "argv": "0.0.2",
    "command-line-args": "^5.1.1",
    "danack-message": "0.0.4",
    "node-sass": "^4.14.1",
    "preact": "^8.5.3",
    "preact-compat": "^3.19.0",
    "preact-render-to-string": "4.1.0",
    "promise-polyfill": "^8.1.3",
    "widgety": "0.0.7"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "compression-webpack-plugin": "^3.0.1",
    "css-loader": "^3.4.2",
    "raw-loader": "^4.0.0",
    "script-loader": "^0.7.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "timestamp-webpack-plugin": "^0.2.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.5",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
